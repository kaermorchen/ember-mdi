{
  "name": "ember-mdi",
  "version": "0.4.5",
  "description": "An ember-cli addon for using Material Design Icons in Ember applications.",
  "keywords": [
    "material design icons",
    "mdi",
    "icons",
    "svg",
    "ember-addon"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:kaermorchen/ember-mdi.git"
  },
  "license": "MIT",
  "author": {
    "name": "Stanislav Romanov",
    "email": "kaermorchen@gmail.com",
    "url": "https://stanislavromanov.ru"
  },
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build",
    "deploy": "ember github-pages:commit --message \"Deploy gh-pages from commit $(git rev-parse HEAD)\"; git push; git checkout -",
    "lint:hbs": "ember-template-lint .",
    "lint:js": "eslint .",
    "start": "ember serve",
    "test": "ember test",
    "test:all": "ember try:each"
  },
  "dependencies": {
    "@mdi/svg": "^3.0.0",
    "broccoli-funnel": "^2.0.1",
    "broccoli-svgstore": "^0.4.2",
    "ember-cli-babel": "^7.4.0",
    "ember-cli-htmlbars": "^3.0.0",
    "ember-cli-htmlbars-inline-precompile": "^1.0.3",
    "ember-cli-sass": "^8.0.0",
    "sass": "^1.16.0"
  },
  "devDependencies": {
    "@ember/jquery": "^0.5.2",
    "@ember/optional-features": "^0.6.3",
    "bootstrap": "^4.1.0",
    "broccoli-asset-rev": "^2.7.0",
    "ember-cli": "~3.7.1",
    "ember-cli-bootstrap-4": "^0.9.1",
    "ember-cli-dependency-checker": "^3.0.0",
    "ember-cli-eslint": "^4.2.3",
    "ember-cli-github-pages": "^0.2.0",
    "ember-cli-inject-live-reload": "^2.0.0",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-template-lint": "^1.0.0-beta.1",
    "ember-cli-uglify": "^2.1.0",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.0",
    "ember-fetch": "^6.4.0",
    "ember-load-initializers": "^1.1.0",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-power-select": "^2.2.0",
    "ember-qunit": "^3.4.1",
    "ember-resolver": "^5.0.1",
    "ember-source": "^3.7.2",
    "ember-source-channel-url": "^1.1.0",
    "ember-spectrum-color-picker": "^0.7.0",
    "ember-truth-helpers": "^2.0.0",
    "ember-try": "^1.0.0",
    "eslint-plugin-ember": "^5.2.0",
    "eslint-plugin-node": "^7.0.1",
    "loader.js": "^4.7.0",
    "qunit-dom": "^0.8.0"
  },
  "engines": {
    "node": "6.* || 8.* || >= 10.*"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "defaultBlueprint": "ember-mdi",
    "demoURL": "https://kaermorchen.github.io/ember-mdi/"
  }
}
