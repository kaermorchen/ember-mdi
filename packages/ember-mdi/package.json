{
  "name": "ember-mdi",
  "version": "0.10.0",
  "description": "An ember-cli addon for using Material Design Icons in Ember applications.",
  "keywords": [
    "material design icons",
    "mdi",
    "icons",
    "svg",
    "ember-addon"
  ],
  "repository": "https://github.com/kaermorchen/ember-mdi",
  "license": "MIT",
  "author": "Stanislav Romanov <kaermorchen@gmail.com>",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "files": [
    "addon-main.js",
    "dist"
  ],
  "scripts": {
    "build": "rollup --config",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel \"lint:!(fix)\"",
    "lint:fix": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*:fix",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "start": "rollup --config --watch",
    "test": "echo 'Addon does not have tests, run tests in test-app'",
    "prepare": "npm run build"
  },
  "dependencies": {
    "@embroider/addon-shim": "^1.6.0",
    "@glimmer/component": "^1.1.2",
    "@mdi/svg": "^6.6.96"
  },
  "devDependencies": {
    "@babel/core": "7.17.9",
    "@babel/plugin-proposal-class-properties": "7.16.7",
    "@babel/plugin-proposal-decorators": "7.17.9",
    "@babel/plugin-syntax-decorators": "7.17.0",
    "@embroider/addon-dev": "1.6.0",
    "@rollup/plugin-babel": "5.3.1",
    "babel-eslint": "10.1.0",
    "ember-template-lint": "4.3.0",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-ember": "10.6.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-qunit": "7.2.0",
    "npm-run-all": "4.1.5",
    "rollup": "2.70.1"
  },
  "engines": {
    "node": "12.* || 14.* || >= 16"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "version": 2,
    "type": "addon",
    "main": "addon-main.js",
    "app-js": {
      "./components/md-icon.js": "./dist/_app_/components/md-icon.js"
    }
  },
  "exports": {
    "./*": "./dist/*",
    "./addon-main.js": "./addon-main.js"
  }
}
